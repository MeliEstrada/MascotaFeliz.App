@page
@model MascotaFeliz.App.Frontend.Pages.EditAdministradorProvisionalModel
@{
}
<h2>Datos el Administrador: @Model.Administrador.Nombre @Model.Administrador.Apellidos</h2>
<form method="post">
    <input type="hidden" asp-for = "Administrador.Id"/>
    <div class="form_group">
        <label asp-for = "Administrador.Nombre"></label>
        <input asp-for = "Administrador.Nombre" class="form-control">
        <span class = "text-danger" asp-validation-for = "Administrador.Nombre"></span>
    </div>
    <div class="form-group">
        <label asp-for = "Administrador.Apellidos"></label>
        <input asp-for = "Administrador.Apellidos" class = "form-control">
        <span class = "text-danger" asp-validation-for = "Administrador.Apellidos"></span>
    </div>
        <div class="form-group">
        <label asp-for = "Administrador.NumeroTelefono"></label>
        <input asp-for = "Administrador.NumeroTelefono" class = "form-control">
        <span class = "text-danger" asp-validation-for = "Administrador.NumeroTelefono"></span>
    </div>
    <div class="form-group">
        <label asp-for = "Administrador.CorreoElectronico"></label>
        <input asp-for = "Administrador.CorreoElectronico" class = "form-control">
        <span class = "text-danger" asp-validation-for = "Administrador.CorreoElectronico"></span>
    </div>
    <button type="submit" class="btn btn-primary">Grabar</button>
</form>
<a asp-page = "./ListAdministradorProvisional" class = "btn-default">Lista de Administradores</a>
